@model Core.DTO.TipoocorrenciaDTO

@{
    ViewData["Title"] = "Remover Tipo Ocorrencia";
    Layout = "_LayoutDashboard";
}

<div>
    <dl class="row">
        <div class="col-sm-2">
            Nº Cadastral
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdTipoOcorrencia)
        </dd>
        <div class="col-sm-2">
            Nome
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <div class="col-sm-2">
            Organização
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeFantasiaOrganizacao)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Remover" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
