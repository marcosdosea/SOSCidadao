@model Core.DTO.TipopertenceDTO

@{
    ViewData["Title"] = "Detalhes Tipo Pertence";
    Layout = "_LayoutDashboard";
}

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Tipo Pertence
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            Nome Organizacao
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeOrganizacao)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdTipoPertence }) |
    <a asp-action="Index">Voltar</a>
</div>